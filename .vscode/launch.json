{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python 调试程序: 包含参数的当前文件",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}"
        },
        {
            "name": "Train PPT ETH",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/train_PPT.py",
            "args": [
                "--dataset_name", "eth",
                "--info", "baseline",
                "--max_epochs", "1000",
                "--learning_rate", "0.0015",
                "--root_path", "2024.12.14",
                "--wandb_group", "2024.12.14eth",
                "--notes", "eth实验",
                "--rotation", "True",
                "--int_num_layers_list", "3", "2",
                "--gpu", "0",
                "--n_head", "8",
                "--lambda_j", "0",
                "--use_image", "True",
                "--use_wandb",
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: Train univ",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/train_PPT.py",
            "args": [
                "--dataset_name",
                "univ",
                "--info",
                "debug_test",
                "--max_epochs",
                "100",
                "--learning_rate",
                "0.0015",
                "--root_path",
                "2024.10.26",
                "--wandb_group",
                "univ",
                "--notes",
                "univ实验",
                "--rotation",
                "True",
                "--int_num_layers_list",
                "1",
                "1",
                "--gpu",
                "0",
                "--use_wandb",
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Train sdd",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/train_PPT.py",
            "args": [
                "--dataset_name",
                "sdd_world",
                "--info",
                "debug_test",
                "--max_epochs",
                "100",
                "--learning_rate",
                "0.0015",
                "--root_path",
                "2024.10.26",
                "--wandb_group",
                "sdd",
                "--notes",
                "sdd实验",
                "--rotation",
                "True",
                "--int_num_layers_list",
                "3",
                "2",
                "--gpu",
                "0",
                "--use_wandb",
                "--batch_size",
                "64"
            ],
            "console": "integratedTerminal"
        }
    ]
}